@model IEnumerable<Pogoda.Models.SmallWeather>

@{
    ViewBag.Title = "Index";
}

<p>
    <h2>Historia pogody</h2>
</p>
<table class="table">
    <tr>
        <th>
            Id wpisu
        </th>
        <th>
            Temperatura
        </th>
        <th>
            Wilgotność
        </th>
        <th>
            Data odczytu
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WeatherReportId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Temperature)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Humidity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOf)
            </td>
            <td>
               @* @Html.ActionLink("Edit", "Edit", new { id = item.ID })
                @Html.ActionLink("Delete", "Delete", new { id = item.ID }) *@
            </td>
        </tr>
    }

</table>

